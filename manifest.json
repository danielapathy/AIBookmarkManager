{
  "manifest_version": 3,
  "name": "AI Bookmark Manager (Clean)",
  "version": "1.0",
  "description": "Intelligent bookmark manager that uses AI to organize, categorize, and suggest bookmarks.",
  "permissions": ["bookmarks", "storage"],
  "host_permissions": ["https://api.anthropic.com/*", "https://api.openai.com/*"],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
   "icons": {
    "128": "icon.png"
  }
}
